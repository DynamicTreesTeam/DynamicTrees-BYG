{
  "multipart": [
    {
      "when": { "distance": "1|2" },
      "apply": { "model": "dtbyg:block/palm_frond_mega" }
    },
    {
      "when": { "distance": "3" },
      "apply": { "model": "dtbyg:block/palm_core_top" }
    },
    {
      "when": { "distance": "4" },
      "apply": { "model": "dtbyg:block/palm_core_bottom" }
    },
    {
      "when": { "OR": [ { "direction": "0", "distance": "1|2" }, { "distance": "5|6|7" } ] },
      "apply": { "model": "byg:block/palm_leaves" }
    }
  ]
}